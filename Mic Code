

#define PORT 3
#include <Arduino.h>
#include <Wire.h>
#include <SoftwareSerial.h>
#define TIMEOUT 30000
#define WAITING_TIME 1000
#define ULTRASONIC 10
#include <MeMCore.h>
#define power 11

MePort MicPort(PORT);
float micLow = 0;
float micHigh = 0;


void setup()
{
  Serial.begin(9600);
}

void loop() {

digitalWrite(power, HIGH);
//voltage from LOW freq
micLow = MicPort.aRead2();
Serial.println(micLow);

//voltage from HIGH freq
micHigh = MicPort.aRead1();
Serial.println(micHigh);

  if ( micLow > micHigh ) {
    //left turn 
    //move(3, 255);
    Serial.println("300hz louder");
    delay(1000);
  } 
  
  if ( micLow < micHigh) {
     //right turn
     //move(4, 255);
     Serial.println("3000hz louder");
     delay(1000);
    }
    
  if (micLow == micHigh) {
  Serial.println("they are equal");
  //move(4,255);
  delay(500);
  //move(1, 255);
}
}
